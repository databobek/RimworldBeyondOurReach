<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <RecipeDef>
        <defName>BOR_AE_Make_Nanoclusters</defName>
        <label>make nanoclusters from nanomachinery</label>
        <description>Make some nanoclusters.</description>
        <jobString>Making nanoclusters.</jobString>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_Machining</soundWorking>
        <workAmount>100000</workAmount>
        <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
        <recipeUsers>
            <li>BasicArchotechWorkbench</li>
        </recipeUsers>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Nanomachinery</li>
                    </thingDefs>
                </filter>
                <count>25</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>Nanomachinery</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <BOR_Nanoclusters>5</BOR_Nanoclusters>
        </products>
        <researchPrerequisite>BOR_Research_Medicine_I</researchPrerequisite>
        <skillRequirements>
            <Crafting>15</Crafting>
        </skillRequirements>
        <workSkill>Crafting</workSkill>
    </RecipeDef>

