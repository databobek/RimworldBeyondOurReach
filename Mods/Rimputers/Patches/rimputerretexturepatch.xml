<?xml version="1.0" encoding="UTF-8"?>
<Patch>

  <!-- Rimputers 1.5 -->
  <Operation Class="BeyondOurReachModSettings.PatchOperationModSettings">
    <modSettingDef>BOR_Setting_RetextureRimputers15QuantumComputer</modSettingDef>
    <enabled Class="PatchOperationReplace">
      <xpath>Defs/ThingDef[defName="Rimputer_quantumcomputer"]/graphicData/texPath</xpath>
      <value>
        <texPath>BOR/Retextures/quantumcomputer</texPath>
      </value>
    </enabled>
  </Operation>
  <Operation Class="BeyondOurReachModSettings.PatchOperationModSettings">
    <modSettingDef>BOR_Setting_RetextureRimputers15QuantumComputer</modSettingDef>
    <enabled Class="PatchOperationReplace">
      <xpath>Defs/ThingDef[defName="Rimputer_quantumcomputer"]/graphicData/graphicClass</xpath>
      <value>
        <graphicClass>Graphic_Single</graphicClass>
      </value>
    </enabled>
  </Operation>
  <Operation Class="BeyondOurReachModSettings.PatchOperationModSettings">
    <modSettingDef>BOR_Setting_RetextureRimputers15QuantumComputer</modSettingDef>
    <enabled Class="PatchOperationReplace">
      <xpath>Defs/ThingDef[defName="Rimputer_quantumcomputer"]/rotatable</xpath>
      <value>
        <rotatable>False</rotatable>
      </value>
    </enabled>
  </Operation>

  <!-- Rimputers No Dubs Bad Hygiene -->
  <Operation Class="BeyondOurReachModSettings.PatchOperationModSettings">
    <modSettingDef>BOR_Setting_RetextureRimputersNODBHQuantumComputer</modSettingDef>
    <enabled Class="PatchOperationReplace">
      <xpath>Defs/ThingDef[defName="Rimputer_quantumcomputer"]/graphicData/texPath</xpath>
      <value>
        <texPath>BOR/Retextures/quantumcomputer</texPath>
      </value>
    </enabled>
  </Operation>
  <Operation Class="BeyondOurReachModSettings.PatchOperationModSettings">
    <modSettingDef>BOR_Setting_RetextureRimputersNODBHQuantumComputer</modSettingDef>
    <enabled Class="PatchOperationReplace">
      <xpath>Defs/ThingDef[defName="Rimputer_quantumcomputer"]/graphicData/graphicClass</xpath>
      <value>
        <graphicClass>Graphic_Single</graphicClass>
      </value>
    </enabled>
  </Operation>
  <Operation Class="BeyondOurReachModSettings.PatchOperationModSettings">
    <modSettingDef>BOR_Setting_RetextureRimputersNODBHQuantumComputer</modSettingDef>
    <enabled Class="PatchOperationReplace">
      <xpath>Defs/ThingDef[defName="Rimputer_quantumcomputer"]/rotatable</xpath>
      <value>
        <rotatable>False</rotatable>
      </value>
    </enabled>
  </Operation>
</Patch>