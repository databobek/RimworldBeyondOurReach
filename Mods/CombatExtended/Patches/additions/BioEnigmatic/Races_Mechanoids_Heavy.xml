<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biotech</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>


				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[@Name="HeavyEnigmaticMechanoid"]/statBases/ArmorRating_Blunt</xpath>
					<value>
						<ArmorRating_Blunt>53</ArmorRating_Blunt>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[@Name="HeavyEnigmaticMechanoid"]/statBases/ArmorRating_Sharp</xpath>
					<value>
						<ArmorRating_Sharp>26</ArmorRating_Sharp>
					</value>
				</li>


				<li Class="PatchOperationAddModExtension">
					<xpath>Defs/ThingDef[defName="BAOTC_Enigmatic_Mech_Tunneler"]</xpath>
					<value>
						<li Class="CombatExtended.RacePropertiesExtensionCE">
							<bodyShape>QuadrupedLow</bodyShape>
						</li>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="BAOTC_Enigmatic_Mech_Tunneler"]/statBases/ArmorRating_Blunt</xpath>
					<value>
						<ArmorRating_Blunt>203</ArmorRating_Blunt>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="BAOTC_Enigmatic_Mech_Tunneler"]/statBases/ArmorRating_Sharp</xpath>
					<value>
						<ArmorRating_Sharp>81</ArmorRating_Sharp>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="BAOTC_Enigmatic_Mech_Tunneler"]/statBases</xpath>
					<value>
						<ArmorRating_Heat>0.843</ArmorRating_Heat>
						<MeleeDodgeChance>0.02</MeleeDodgeChance>
						<MeleeCritChance>0.23</MeleeCritChance>
						<MeleeParryChance>0.53</MeleeParryChance>
						<MaxHitPoints>1013</MaxHitPoints>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="BAOTC_Enigmatic_Mech_Tunneler"]/statBases/EnergyShieldEnergyMax</xpath>
					<value>
						<EnergyShieldEnergyMax>12.657</EnergyShieldEnergyMax>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="BAOTC_Enigmatic_Mech_Tunneler"]/tools</xpath>
					<value>
						<tools>
							<li Class="CombatExtended.ToolCE">
								<label>left power claw</label>
								<capacities>
									<li>Blunt</li>
								</capacities>
								<power>45</power>
								<cooldownTime>2.182</cooldownTime>
								<linkedBodyPartsGroup>LeftPowerClaw</linkedBodyPartsGroup>
								<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
								<armorPenetrationBlunt>68</armorPenetrationBlunt>
							</li>
							<li Class="CombatExtended.ToolCE">
								<label>right power claw</label>
								<capacities>
									<li>Scratch</li>
								</capacities>
								<power>36</power>
								<cooldownTime>1.835</cooldownTime>
								<linkedBodyPartsGroup>RightPowerClaw</linkedBodyPartsGroup>
								<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
								<armorPenetrationSharp>8</armorPenetrationSharp>
								<armorPenetrationBlunt>35</armorPenetrationBlunt>
							</li>
						</tools>
					</value>
				</li>

				<li Class="PatchOperationConditional">
					<xpath>Defs/ThingDef[defName="BAOTC_Enigmatic_Mech_Tunneler"]/comps</xpath>
					<nomatch Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName="BAOTC_Enigmatic_Mech_Tunneler"]</xpath>
						<value>
							<comps />
						</value>
					</nomatch>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="BAOTC_Enigmatic_Mech_Tunneler"]/comps</xpath>
					<value>
						<li Class="CombatExtended.CompProperties_ArmorDurability">
							<Durability>2500</Durability>
							<Regenerates>true</Regenerates>
							<RegenInterval>1250</RegenInterval>
							<RegenValue>5</RegenValue>
							<Repairable>true</Repairable>
							<RepairIngredients>
								<SNS_CosmicAlloy>5</SNS_CosmicAlloy>
								<SNS_Eternium>5</SNS_Eternium>
							</RepairIngredients>
							<RepairTime>300</RepairTime>
							<RepairValue>200</RepairValue>
							<CanOverHeal>true</CanOverHeal>
							<MaxOverHeal>250</MaxOverHeal>
							<MinArmorPct>0.75</MinArmorPct>
						</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>

</Patch>