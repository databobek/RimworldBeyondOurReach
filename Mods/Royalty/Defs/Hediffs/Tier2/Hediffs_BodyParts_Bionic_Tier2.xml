  <!-- Gastro-analyzer -->

  <HediffDef ParentName="ImplantHediffBase">
    <defName>GastroAnalyzerQuasar</defName>
    <label>kwazaari gastro-analyzer</label>
    <labelNoun>a kwazaari gastro-analyzer</labelNoun>
    <description>An installed kwazaari gastro-analyzer.</description>
