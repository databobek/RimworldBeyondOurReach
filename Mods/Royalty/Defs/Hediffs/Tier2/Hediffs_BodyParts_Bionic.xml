    <jobString>Installing quasar neurocalculator.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>NeurocalculatorQuasar</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>NeurocalculatorQuasar</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Brain</li>
    </appliedOnFixedBodyParts>
    <addsHediff>NeurocalculatorQuasar</addsHediff>
    <incompatibleWithHediffTags>
