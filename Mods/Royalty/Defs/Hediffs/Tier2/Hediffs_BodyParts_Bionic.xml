<Defs>

  <!-- Neurocalculator -->

  <HediffDef ParentName="BOR_ImplantQuasarHediffBase">
    <defName>NeurocalculatorQuasar</defName>
    <label>quasar neurocalculator</label>
    <labelNoun>a quasar neurocalculator</labelNoun>
