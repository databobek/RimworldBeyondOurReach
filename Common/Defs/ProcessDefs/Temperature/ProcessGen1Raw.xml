<Defs>

  <PipeSystem.ProcessDef Abstract="True" Name="BOR_ProcessGenITemp">
    <destroyIngredientsDirectly>true</destroyIngredientsDirectly>
    <temperatureRuinable>true</temperatureRuinable>
    <minSafeTemperature>0.0</minSafeTemperature>
    <maxSafeTemperature>15.0</maxSafeTemperature>
    <researchPrerequisites>
      <li>BOR_Research_NSynth_I</li>
    </researchPrerequisites>
  </PipeSystem.ProcessDef>

  <PipeSystem.ProcessDef ParentName="BOR_ProcessGenITemp">
    <defName>BOR_NeutroniumIntoSteelGenITempRuin</defName>
    <label>Fabricate Steel from Neutronium in safe temperature (0-15*C)</label>
    <ingredients>
      <li>
        <pipeNet>BOR_NeutroniumNet</pipeNet>
        <thing>BOR_Neutronium</thing>
        <countNeeded>7500</countNeeded>
      </li>
      <li>
        <thing>Steel</thing>
        <countNeeded>1</countNeeded>
      </li>
    </ingredients>
    <ticks>1500000</ticks>
    <results>
      <li>
        <thing>Steel</thing>
        <count>500</count>
      </li>
    </results>
  </PipeSystem.ProcessDef>


