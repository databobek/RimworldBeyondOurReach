  <PipeSystem.ProcessDef ParentName="BOR_ProcessGenITemp">
    <defName>BOR_NeutroniumIntoBOR_NanoclustersGenITempRuin</defName>
    <label>Fabricate Nanoclusters from Neutronium in safe temperature (0-15*C)</label>
    <ingredients>
      <li>
        <pipeNet>BOR_NeutroniumNet</pipeNet>
        <thing>BOR_Neutronium</thing>
        <countNeeded>520000</countNeeded>
      </li>
      <li>
        <thing>BOR_Nanoclusters</thing>
