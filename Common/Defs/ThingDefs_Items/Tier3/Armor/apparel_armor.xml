  <ThingDef ParentName="ArmorMachineableBase">
    <defName>BOR_Tenebral_Apparel_FlakVest</defName>
    <label>tenebral flak vest</label>
    <description>Two plates of Tenebrite Crystalline Mesh riveted together to create the pinnacle of torso protection. This protective vest can withstand being hit by a railway gun without so much as a dent, though the same cannot be said for the wearer's unprotected limbs.</description>
    <techLevel>Archotech</techLevel>
    <recipeMaker>
      <researchPrerequisites>
        <li>BOR_Research_Armor_III</li>
        <li>FlakArmor</li>
      </researchPrerequisites>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
      <skillRequirements>
        <Crafting>9</Crafting>
      </skillRequirements>
      <displayPriority>220</displayPriority>
      <recipeUsers Inherit="False">
        <li>BOR_Bench_III</li>
        <li>BOR_Bench_IV</li>
      </recipeUsers>
    </recipeMaker>
    <graphicData>

      <color>(0.2, 0.2, 0.2)</color>
      <texPath>Things/Pawn/Humanlike/Apparel/FlakVest/FlakVest</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>0.90</drawSize>
    </graphicData>
    <statBases>
      <WorkToMake>20250</WorkToMake>
      <MaxHitPoints>675</MaxHitPoints>
      <Mass>1.8</Mass>
      <Flammability>0.3</Flammability>
      <ArmorRating_Sharp>3.375</ArmorRating_Sharp>
      <ArmorRating_Blunt>1.35</ArmorRating_Blunt>
      <ArmorRating_Heat>0.945</ArmorRating_Heat>
      <Insulation_Cold>4.5</Insulation_Cold>
      <EquipDelay>3</EquipDelay>
    
			<RoyalFavorValue>6</RoyalFavorValue></statBases>
    <equippedStatOffsets>
      <MoveSpeed>-0.04</MoveSpeed>
    </equippedStatOffsets>
    <thingCategories>
      <li>BOR_Armor_III</li>
    </thingCategories>
    <costList>
      <BOR_Tenebrite>90</BOR_Tenebrite>
      <BOR_ComponentIII>1</BOR_ComponentIII>
    </costList>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Pawn/Humanlike/Apparel/FlakVest/FlakVest</wornGraphicPath>
      <layers>
        <li>Middle</li>
      </layers>
      <tags>
        <li>BOR_Military_III</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <tradeTags>
      <li>BOR_Armor_III</li>
			<li>BOR_Tech_III</li>
    </tradeTags>
  </ThingDef>

